{
  "manifest_version": 3,

  "name": "Job application tracker",
  "description": "Save your job applications",
  "version": "1.0.0",
  "action": {
    "default_icon": "logo192.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab", "scripting"
  ],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content_script.js"]
  }]
}